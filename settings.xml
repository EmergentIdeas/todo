<?xml version="1.0"  ?>
<ivysettings>
	<include url="${ivy.default.settings.dir}/ivysettings-public.xml" />
	<include url="${ivy.default.settings.dir}/ivysettings-shared.xml" />
	<include url="${ivy.default.settings.dir}/ivysettings-local.xml" />
	<include url="${ivy.default.settings.dir}/ivysettings-main-chain.xml" />
	<include url="${ivy.default.settings.dir}/ivysettings-default-chain.xml" />

	<settings defaultResolver="eico-chain" />
	
	<resolvers>
		<url name="com.emergentideas.public">
			<ivy pattern="https://raw.github.com/EmergentIdeas/ivyrepo/master/release/[organisation]/[module]/[revision]/[artifact]-[revision].[ext]" />
			<artifact pattern="https://raw.github.com/EmergentIdeas/ivyrepo/master/release/[organisation]/[module]/[revision]/[artifact]-[revision].[ext]" />
		</url>

		<chain name="eico-chain">
			<reslover ref="public" />
			<reslover ref="com.emergentideas.public" />
			<ibiblio name="ibiblio" m2compatible="true" />
		</chain>
	</resolvers>
</ivysettings>